{
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claudemol/bin/claudemol status 2>/dev/null || echo 'claudemol: not configured (run /pymol-setup)'",
            "timeout": 10
          }
        ]
      }
    ]
  }
}
